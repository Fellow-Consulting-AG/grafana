        {
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "dega8v6bs6mm8d"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  COUNT(d.id) AS value, \n  SUBSTRING(oc.name FROM 1 FOR 5) || ' - ' || d.status AS metric,\n  STRING_AGG(d.id::text, ', ') AS document_ids\nFROM documents d\nJOIN organisation_cache oc ON d.org_id = oc.id\nWHERE d.status IN (\n    'new',\n    'restarted',\n    'restart_export',\n    'restart_in_progress',\n    'running',\n    'validating',\n    'validated_pending_export',\n    'approved_pending_export'\n)\nAND d.is_deleted = false\nAND d.last_modified_on < NOW() - INTERVAL '45 minutes'\nAND d.created_on BETWEEN $__timeFrom() AND $__timeTo()\nGROUP BY metric\nORDER BY metric ASC;\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "name": "COUNT",
                "parameters": [
                  {
                    "name": "id",
                    "type": "functionParameter"
                  }
                ],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          },
          "table": "documents"
        }
      ],
      "title": "Stuck Documents",
      "transparent": true,

{
  "dashboard": {
    "id": null,
    "title": "Kubernetes Egress Gateway Monitoring",
    "tags": [
      "kubernetes",
      "egress",
      "networking",
      "gateway"
    ],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Egress Gateway Status",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "k8s_egress_gateway_reachable{gateway=\"64.226.113.148\"}",
            "legendFormat": "{{node}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ],
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "Gateway Availability %",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "targets": [
          {
            "expr": "avg(k8s_egress_gateway_reachable) * 100",
            "legendFormat": "Availability",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 95
                },
                {
                  "color": "green",
                  "value": 99
                }
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 3,
        "title": "Total Egress Connections",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(k8s_egress_connections_total)",
            "legendFormat": "Total Connections",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        }
      },
      {
        "id": 4,
        "title": "Total External Connections",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(k8s_egress_external_connections_total)",
            "legendFormat": "External Connections",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        }
      },
      {
        "id": 5,
        "title": "Gateway Reachability Timeline",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "k8s_egress_gateway_reachable{gateway=\"64.226.113.148\"}",
            "legendFormat": "{{node}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "stepAfter",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "min": 0,
            "max": 1,
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": [
              "lastNotNull",
              "mean"
            ]
          }
        }
      },
      {
        "id": 6,
        "title": "Egress Connections Over Time",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum by (node) (k8s_egress_connections_total)",
            "legendFormat": "{{node}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 0,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "min": 0,
            "unit": "short"
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 7,
        "title": "Top 10 Nodes by Egress Usage",
        "type": "barchart",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "topk(10, k8s_egress_connections_total)",
            "legendFormat": "{{node}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 80,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "min": 0,
            "unit": "short"
          }
        },
        "options": {
          "orientation": "horizontal",
          "xTickLabelRotation": 0,
          "xTickLabelSpacing": 0,
          "legend": {
            "displayMode": "hidden"
          },
          "tooltip": {
            "mode": "single"
          }
        }
      },
      {
        "id": 8,
        "title": "External vs Egress Connections",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "expr": "sum(k8s_egress_external_connections_total)",
            "legendFormat": "Total External Connections",
            "refId": "A"
          },
          {
            "expr": "sum(k8s_egress_connections_total)",
            "legendFormat": "Connections via Egress Gateway",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "min": 0,
            "unit": "short"
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": [
              "lastNotNull",
              "max",
              "mean"
            ]
          }
        }
      },
      {
        "id": 9,
        "title": "Node Status Table",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        },
        "targets": [
          {
            "expr": "k8s_egress_gateway_reachable",
            "format": "table",
            "refId": "A"
          },
          {
            "expr": "k8s_egress_connections_total",
            "format": "table",
            "refId": "B"
          },
          {
            "expr": "k8s_egress_external_connections_total",
            "format": "table",
            "refId": "C"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {
              "reducers": []
            }
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "Time": true,
                "gateway": false,
                "instance": true,
                "job": true
              },
              "indexByName": {
                "node": 0,
                "Value #A": 1,
                "Value #B": 2,
                "Value #C": 3
              },
              "renameByName": {
                "Value #A": "Gateway Reachable",
                "Value #B": "Egress Connections",
                "Value #C": "External Connections",
                "node": "Node"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto",
              "inspect": false
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "❌ DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "✅ UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Gateway Reachable"
              },
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "custom.align",
                  "value": "center"
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "desc": true,
              "displayName": "Egress Connections"
            }
          ]
        }
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "name": "Gateway Down",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "enable": true,
          "expr": "changes(k8s_egress_gateway_reachable[5m]) > 0 and k8s_egress_gateway_reachable == 0",
          "iconColor": "red",
          "name": "Gateway Down",
          "titleFormat": "Egress Gateway Unavailable",
          "textFormat": "Node {{node}} cannot reach egress gateway {{gateway}}"
        }
      ]
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 0,
    "links": [],
    "liveNow": false,
    "panels": [],
    "schemaVersion": 37,
    "style": "dark",
    "tags": [
      "kubernetes",
      "egress",
      "networking"
    ],
    "templating": {
      "list": []
    },
    "timepicker": {},
    "timezone": "",
    "title": "Kubernetes Egress Gateway Monitoring",
    "uid": "egress-gateway-monitoring",
    "version": 1,
    "weekStart": ""
  }
}
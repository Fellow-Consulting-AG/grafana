{
  "id": null,
  "title": "üìä DocBits API Performance - User-Friendly Dashboard (Fixed)",
  "tags": ["docbits", "api", "performance", "monitoring", "production"],
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "namespace",
        "type": "query",
        "query": "label_values(nginx_ingress_controller_requests, namespace)",
        "current": {
          "value": "All",
          "text": "All"
        },
        "multi": false,
        "includeAll": true,
        "refresh": "on_dashboard_load",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        }
      },
      {
        "name": "ingress",
        "type": "query", 
        "query": "label_values(nginx_ingress_controller_requests{namespace=~\"$namespace\"}, ingress)",
        "current": {
          "value": "api",
          "text": "api"
        },
        "multi": true,
        "includeAll": true,
        "refresh": "on_dashboard_load",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "üö® CRITICAL API STATUS",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "‚ö° Requests per Second",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 100},
              {"color": "red", "value": 500}
            ]
          }
        }
      },
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 0,
        "y": 1
      },
      "targets": [
        {
          "expr": "sum(rate(nginx_ingress_controller_requests{namespace=~\"$namespace\", ingress=~\"$ingress\"}[1m]))",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "title": "üéØ Success Rate (Last 5min)",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100,
          "min": 0,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": 0},
              {"color": "orange", "value": 95},
              {"color": "yellow", "value": 98},
              {"color": "green", "value": 99.5}
            ]
          }
        }
      },
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 4,
        "y": 1
      },
      "targets": [
        {
          "expr": "(1 - (sum(rate(nginx_ingress_controller_requests{namespace=~\"$namespace\", ingress=~\"$ingress\", status=~\"5..\"}[5m])) / sum(rate(nginx_ingress_controller_requests{namespace=~\"$namespace\", ingress=~\"$ingress\"}[5m])))) * 100",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "title": "‚è±Ô∏è Response Time P50/P95/P99",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 200},
              {"color": "orange", "value": 1000},
              {"color": "red", "value": 3000}
            ]
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "P50"},
            "properties": [{"id": "displayName", "value": "P50 (Median)"}]
          },
          {
            "matcher": {"id": "byName", "options": "P95"},
            "properties": [{"id": "displayName", "value": "P95 (95th percentile)"}]
          },
          {
            "matcher": {"id": "byName", "options": "P99"},
            "properties": [{"id": "displayName", "value": "P99 (99th percentile)"}]
          }
        ]
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 1
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(nginx_ingress_controller_request_duration_seconds_bucket{namespace=~\"$namespace\", ingress=~\"$ingress\"}[5m])) by (le)) * 1000",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(nginx_ingress_controller_request_duration_seconds_bucket{namespace=~\"$namespace\", ingress=~\"$ingress\"}[5m])) by (le)) * 1000", 
          "legendFormat": "P95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(nginx_ingress_controller_request_duration_seconds_bucket{namespace=~\"$namespace\", ingress=~\"$ingress\"}[5m])) by (le)) * 1000",
          "legendFormat": "P99",
          "refId": "C"
        }
      ]
    },
    {
      "id": 5,
      "title": "üî• Active Pod Count",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": 0},
              {"color": "yellow", "value": 1},
              {"color": "green", "value": 2}
            ]
          }
        }
      },
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 16,
        "y": 1
      },
      "targets": [
        {
          "expr": "sum(kube_deployment_status_replicas{deployment=\"api\", namespace=~\"$namespace\"})",
          "refId": "A"
        }
      ]
    },
    {
      "id": 6,
      "title": "üìà RESPONSE TIME TRENDS",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 7
      }
    },
    {
      "id": 7,
      "title": "‚è∞ Response Time Over Time (P50/P95/P99)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 0.1,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "axisColorMode": "text",
            "scaleDistribution": {
              "type": "linear"
            },
            "axisCenteredZero": false,
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "red", "value": 3000}
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(nginx_ingress_controller_request_duration_seconds_bucket{namespace=~\"$namespace\", ingress=~\"$ingress\"}[1m])) by (le)) * 1000",
          "legendFormat": "P50 (Median)",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(nginx_ingress_controller_request_duration_seconds_bucket{namespace=~\"$namespace\", ingress=~\"$ingress\"}[1m])) by (le)) * 1000",
          "legendFormat": "P95 (95th percentile)",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(nginx_ingress_controller_request_duration_seconds_bucket{namespace=~\"$namespace\", ingress=~\"$ingress\"}[1m])) by (le)) * 1000",
          "legendFormat": "P99 (99th percentile)",
          "refId": "C"
        }
      ]
    },
    {
      "id": 15,
      "title": "üìä NEW: Endpoint Performance Overview (Avg/Min/Max Times)",
      "type": "barchart",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "bars",
            "barAlignment": 0,
            "barWidthFactor": 0.6,
            "barMaxWidth": 100,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            }
          },
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 2},
              {"color": "orange", "value": 10},
              {"color": "red", "value": 30}
            ]
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "avg_time"},
            "properties": [
              {"id": "displayName", "value": "Avg Response Time"},
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "min_time"},
            "properties": [
              {"id": "displayName", "value": "Min Response Time"},
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "max_time"},
            "properties": [
              {"id": "displayName", "value": "Max Response Time"},
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "topk(10, avg by (path) (avg_over_time({namespace=\"ingress-nginx\"} | pattern `<_> - - <_> \"<method> <path> <_>\" <status> <_> \"<_>\" \"<_>\" <_> <duration> [<service>] [<_>] <_> <_> <_> <_> <_>` | service =~ \".*api.*\" | duration != \"\" | unwrap duration [15m])))",
          "legendFormat": "{{path}} - Avg",
          "refId": "A"
        },
        {
          "expr": "topk(10, min by (path) (min_over_time({namespace=\"ingress-nginx\"} | pattern `<_> - - <_> \"<method> <path> <_>\" <status> <_> \"<_>\" \"<_>\" <_> <duration> [<service>] [<_>] <_> <_> <_> <_> <_>` | service =~ \".*api.*\" | duration != \"\" | unwrap duration [15m])))",
          "legendFormat": "{{path}} - Min",
          "refId": "B"
        },
        {
          "expr": "topk(10, max by (path) (max_over_time({namespace=\"ingress-nginx\"} | pattern `<_> - - <_> \"<method> <path> <_>\" <status> <_> \"<_>\" \"<_>\" <_> <duration> [<service>] [<_>] <_> <_> <_> <_> <_>` | service =~ \".*api.*\" | duration != \"\" | unwrap duration [15m])))",
          "legendFormat": "{{path}} - Max",
          "refId": "C"
        }
      ],
      "transformations": [
        {
          "id": "sortBy",
          "options": {
            "sort": [
              {
                "field": "avg_time",
                "desc": true
              }
            ]
          }
        }
      ]
    },
    {
      "id": 16,
      "title": "üè¢ NEW: Request Times by Organization (x-org-id Header)",
      "type": "timeseries",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 0.2,
            "gradientMode": "opacity",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "auto",
            "pointSize": 3,
            "stacking": {
              "mode": "none",
              "group": "A"
            }
          },
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 2},
              {"color": "orange", "value": 10},
              {"color": "red", "value": 30}
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 24
      },
      "targets": [
        {
          "expr": "avg by (org_id) (avg_over_time({namespace=\"ingress-nginx\"} | pattern `<_> - - <_> \"<method> <path> <_>\" <status> <_> \"<_>\" \"<user_agent>\" <_> <duration> [<service>] [<_>] <_> <_> <_> <_> <org_id>` | service =~ \".*api.*\" | org_id != \"\" | unwrap duration [2m]))",
          "legendFormat": "Org ID: {{org_id}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 8,
      "title": "üö® SMART SLOW REQUEST ANALYSIS",
      "type": "row",
      "collapsed": false,
